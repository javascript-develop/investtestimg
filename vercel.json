{
      "builds": [
        {
          "src": "index.js",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
      ],
      "rewrites": [
        {
          "source": "/api/v1/(.*)",
          "destination": "https://investtestimg.vercel.app/api/v1/$1"
        }
      ],
      "cors": {
        "origin": "http://localhost:3000",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "allowedHeaders": ["Content-Type", "Authorization"],
        "credentials": true
      }
    }
    